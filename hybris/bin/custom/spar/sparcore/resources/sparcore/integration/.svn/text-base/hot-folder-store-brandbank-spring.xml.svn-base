<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:int="http://www.springframework.org/schema/integration"
	xmlns:file="http://www.springframework.org/schema/integration/file"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/integration 
	http://www.springframework.org/schema/integration/spring-integration.xsd
	http://www.springframework.org/schema/integration/file 
	http://www.springframework.org/schema/integration/file/spring-integration-file.xsd
	http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/> 

	<bean id="baseDirectoryBrandbank" class="java.lang.String">
		<constructor-arg value="#{baseDirectory}/${tenantId}/brandbank" />
	</bean>
	
	<!-- General Setup Start -->
	<bean id="fileOrderComparatorBrandBank" class="de.hybris.platform.acceleratorservices.dataimport.batch.FileOrderComparator">
		<property name="prefixPriority">
			<map>
				<entry key="ProductMediaFormat" value="4" />
				<entry key="ProductMedias" value="3" />
				<entry key="ProductCategoryFeatures" value="2" />
				<entry key="ProductAttributes" value="1" />
				<!-- <entry key="ProductAttributeItemPickingTime" value="1" /> --> <!-- commented for itempickingtime -->
			</map>
		</property>
	</bean>
	<!-- General Setup End -->
	
	<!-- 1) Scan for files -->
	
	<!-- Allowing duplicate files START -->
	<!--  <file:inbound-channel-adapter id="batchFilesBrandbank" directory="#{baseDirectoryBrandbank}"
		filename-regex="^(.*)_(\d+)\.csv" comparator="fileOrderComparator" prevent-duplicates="false">
		<int:poller fixed-rate="1000" />
	</file:inbound-channel-adapter>
	-->
	<!-- Allowing duplicate files END -->
	
	<!-- Preventing duplicate files START -->
	
	<bean id="batchSequenceIdParser" class="de.hybris.platform.acceleratorservices.dataimport.batch.util.SequenceIdParser">
		<property name="parser">
			<bean class="de.hybris.platform.acceleratorservices.util.RegexParser">
				<property name="regex" value="_(\d+)\.csv" />
			</bean>
		</property>
	</bean>
	<bean id="batchLanguageParser" class="de.hybris.platform.acceleratorservices.util.RegexParser">
		<property name="regex" value="-(\w{2})_(\d+)\.csv" />
	</bean>
	
	
	<file:inbound-channel-adapter id="batchFilesBrandbank" directory="#{baseDirectoryBrandbank}"
		filename-regex="^(.*)_(\d+)\.csv" comparator="fileOrderComparatorBrandBank">
		<int:poller fixed-rate="1000" />
	</file:inbound-channel-adapter>
	<!-- Preventing duplicate files END -->
	
	<!-- 2) move the file to processing and create header -->
	<file:outbound-gateway request-channel="batchFilesBrandbank" reply-channel="batchFilesBrandbankProc"
		directory="#{baseDirectoryBrandbank}/processing" delete-source-files="true" />
	<int:service-activator input-channel="batchFilesBrandbankProc" output-channel="batchFilesHeaderInit" ref="brandbankHeaderSetupTask"
		method="execute" />
	<bean id="brandbankHeaderSetupTask" class="de.hybris.platform.acceleratorservices.dataimport.batch.task.HeaderSetupTask">
		<property name="catalog" value="sparProductCatalog" />
		<property name="net" value="false" />
		<property name="storeBaseDirectory" ref="baseDirectoryBrandbank" />
	</bean>


<!--  Product Category feature impex configuration STARTS -->
	
	<!--  Classifying Category 100 (Product Details) START -->
	<!-- Transformer converters mappings -->
	<bean id="batchBrandbankProductDetailsConverterMapping" 
		  class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.mapping.impl.DefaultConverterMapping" 
		  p:mapping="ProductCategoryFeatures" 
		  p:converter-ref="batchBrandbankProductDetailsConverter"/>
	
	<!-- Brandbank specific converters -->
	<bean id="batchBrandbankProductDetailsConverter" class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.impl.DefaultImpexConverter">
		<property name="header">
			<value>#{defaultImpexProductHeader}
				# Macros / Replacement Parameter definitions
				$clAttrModifiers=system='SparClassification',version='2.0',translator=de.hybris.platform.catalog.jalo.classification.impex.ClassificationAttributeTranslator,lang=en
				
				# Insert Product Classifications
				$productCatalog=sparProductCatalog
                $catalogVersion=catalogversion(catalog(id[default=$productCatalog]),version[default='Staged'])[unique=true,default=$productCatalog:Staged]
	             				
				# Classification: Product Details, 100
				$feature1=@Further Descrption[$clAttrModifiers];  
				$feature2=@Description[$clAttrModifiers];  
				$feature3=@Regional Name[$clAttrModifiers];
				
				# Insert Brandbank Products
                      
				UPDATE Product;code[unique=true];$feature1;$feature2;$feature3;$catalogVersion
			</value>
		</property>
		<property name="impexRow">
			<value>;{+0};{1};{2};{3}</value>
		</property>
	</bean>
	
