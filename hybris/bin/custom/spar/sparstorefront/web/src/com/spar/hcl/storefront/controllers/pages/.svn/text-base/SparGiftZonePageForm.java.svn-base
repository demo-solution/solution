package com.spar.hcl.storefront.controllers.pages;

/**
 * @author pavan.sr
 *
 */
public class SparGiftZonePageForm
{
	private String customerName;
	private String customerEmail;
	private String customerContact;
	private String companyName;
	private String customerCity;
	private String customerRemarks;

	/**
	 * @return the customerName
	 */
	public String getCustomerName()
	{
		return customerName;
	}

	/**
	 * @param customerName
	 *           the customerName to set
	 */
	public void setCustomerName(final String customerName)
	{
		this.customerName = customerName;
	}

	/**
	 * @return the customerEmail
	 */
	public String getCustomerEmail()
	{
		return customerEmail;
	}

	/**
	 * @param customerEmail
	 *           the customerEmail to set
	 */
	public void setCustomerEmail(final String customerEmail)
	{
		this.customerEmail = customerEmail;
	}

	/**
	 * @return the customerContact
	 */
	public String getCustomerContact()
	{
		return customerContact;
	}

	/**
	 * @param customerContact
	 *           the customerContact to set
	 */
	public void setCustomerContact(final String customerContact)
	{
		this.customerContact = customerContact;
	}

	/**
	 * @return the companyName
	 */
	public String getCompanyName()
	{
		return companyName;
	}

	/**
	 * @param companyName
	 *           the companyName to set
	 */
	public void setCompanyName(final String companyName)
	{
		this.companyName = companyName;
	}

	/**
	 * @return the customerCity
	 */
	public String getCustomerCity()
	{
		return customerCity;
	}

	/**
	 * @param customerCity
	 *           the customerCity to set
	 */
	public void setCustomerCity(final String customerCity)
	{
		this.customerCity = customerCity;
	}

	/**
	 * @return the customerRemarks
	 */
	public String getCustomerRemarks()
	{
		return customerRemarks;
	}

	/**
	 * @param customerRemarks
	 *           the customerRemarks to set
	 */
	public void setCustomerRemarks(final String customerRemarks)
	{
		this.customerRemarks = customerRemarks;
	}


}
